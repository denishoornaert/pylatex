<?
def reformat(string):
    return string[2:]

ex = [12, 78, 24, 9, 152]
out("%"+str(ex))
?>

\section{Correction exercice 1}
\begin{center}
\begin{tabular}{|c|c|c|}
\hline
dec & bin & hex \\
\hline
<?
for e in ex:
    out(str(e)+" & "+reformat(bin(e))+" & "+reformat(hex(e))+" \\ \n")
    out("\hline \n")
?>
\end{tabular}
\end{center}

